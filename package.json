{
  "name": "chess-victoria-website",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "algolia:sync": "curl -X POST http://localhost:3000/api/algolia-sync -H \"x-secret-key: test\"",
    "dev": "NEXT_RUN_LOCAL=true next dev  --experimental-https",
    "build": "next build",
    "start": "next start",
    "e2e": "playwright test",
    "lint": "next lint",
    "sass": "sass --watch public/assets/scss/main.scss:public/assets/css/main.css",
    "contentful:login": "contentful login",
    "contentful:space": "contentful space use --space-id $CONTENTFUL_SPACE_ID",
    "contentful:environment": "contentful environment use --environment-id master",
    "contentful:push": "contentful space import --config-file contentful/config.json",
    "contentful:pull": "contentful space export --config-file contentful/config.json --export-dir contentful/exports",
    "contentful:generate": "contentful space generate migration --config-file contentful/config.json",
    "contentful:apply": "contentful space migration --config-file contentful/config.json"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^16.1.0",
    "@contentful/rich-text-types": "^17.1.0",
    "@react-email/render": "1.2.0",
    "@vercel/analytics": "1.5.0",
    "@vercel/speed-insights": "1.2.0",
    "@vis.gl/react-google-maps": "^1.5.5",
    "adm-zip": "^0.5.16",
    "algoliasearch": "^5.35.0",
    "aos": "^2.3.4",
    "contentful": "^11.7.15",
    "contentful-management": "^11.54.4",
    "dotenv": "^17.2.1",
    "gsap": "^3.12.5",
    "next": "^15.4.7",
    "react": "^19.1.1",
    "react-countup": "^6.5.3",
    "react-dom": "^19.1.1",
    "react-markdown": "10.1.0",
    "react-slick": "^0.30.2",
    "remark-gfm": "4.0.1",
    "resend": "^6.0.1",
    "sass": "^1.81.0",
    "slick-carousel": "^1.8.1",
    "swiper": "^11.1.14",
    "wowjs": "^1.1.3"
  },
  "devDependencies": {
    "@playwright/test": "1.55.0",
    "@types/adm-zip": "^0.5.7",
    "@types/algoliasearch": "^3.34.11",
    "@types/aos": "^3.0.7",
    "@types/google.maps": "^3.58.1",
    "@types/node": "^22",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-slick": "^0.23.13",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.4.7",
    "typescript": "^5"
  }
}
